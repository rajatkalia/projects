key "dev.physicslab.org"
literal "true"

\end_inset


\end_layout

\begin_layout Standard
A 5.0-kg mass is pushed along a straight line by a net force described in
 the graph below.
 The object is at rest at t = 0 and x = 0.
\end_layout

\begin_layout Standard
\begin_inset Box Doublebox
position "t"
hor_pos "c"
has_inner_box 1
inner_pos "t"
use_parbox 0
use_makebox 0
width "72col%"
special "none"
height "1in"
height_special "totalheight"
thickness "0.4pt"
separation "3pt"
shadowsize "4pt"
